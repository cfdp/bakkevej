:@template"u(/* This file will hold styles for the mobile version of your website (mobile first). */
/* This also can include ANY global CSS that applies site-wide. Unless overwritten by a more specific style rule, CSS declarations in global.css will apply site-wide. */

@import "base";
@import "text";
@import "forms";
@import "retina-sprites";

body {
	background: $color-red-bg url("../images/bg-pattern-streg.png") repeat;
	@include background-size(9px 1x);
	font-family: $font-fam-std; font-size: $font-size-std; color: #FFFFFF;
	-webkit-text-size-adjust: none;
}
img {
	max-width: 100%;
}

/**
 * MAIN SECTIONS
 */
.section-header {
	.region-branding, .region-header-first {
		max-width: 1024px;
		margin: auto;
		.region-branding-inner, .region-header-first-inner {
			margin: 0 15px;
		}
	}
}
.section-content {
	margin: 20px auto;
	max-width: 1024px;
	.region-content {
		position: inherit;
		.region-content-inner { 
			position: inherit;
		}
	}
}
.section-footer {
	margin: auto;
	max-width: 1024px;
}

/**
 * ICONS - Sprites
 */
$sprites: sprite-map("sprites/red/menu/*.png");
$sprites2x: sprite-map("sprites-retina/red/menu/*.png");

.menu-calendar {
	@include retina-sprite(calendar);
}
.menu-home {
	@include retina-sprite(home);
}
.menu-settings {
	@include retina-sprite(settings);
}
.menu-video {
	@include retina-sprite(video);
}

$sprites: sprite-map("sprites/red/pictograms_title/*.png");
$sprites2x: sprite-map("sprites-retina/red/pictograms_title/*.png");

.icon-calendar-title {
	@include retina-sprite(calendar);
}
.icon-video-title {
	@include retina-sprite(video);
}
.icon-settings-title {
	@include retina-sprite(settings);
}
.icon-plus-title {
	@include retina-sprite(plus);
}
.icon-back-title {
	@include retina-sprite(back);
}

$sprites: sprite-map("sprites/red/pictograms_small/*.png");
$sprites2x: sprite-map("sprites-retina/red/pictograms_small/*.png");

.icon-bad-small {
	@include retina-sprite(bad);
}
.icon-medicin-small {
	@include retina-sprite(medicin);
}
.icon-morgenmad-small {
	@include retina-sprite(morgenmad);
}

$sprites: sprite-map("sprites/red/pictograms_large/*.png");
$sprites2x: sprite-map("sprites-retina/red/pictograms_large/*.png");

.icon-bad-large {
	@include retina-sprite(bad);
}
.icon-medicin-large {
	@include retina-sprite(medicin);
}
.icon-morgenmad-large {
	@include retina-sprite(morgenmad);
}

.picto {
	display: block; float: left;
	margin: 0 5px 0 0;
}
.picto-title {
	display: block; float: left;
	margin: 0 10px 0 0;
}

/**
 * HEADER - Menu
 */
.zone-branding {
	height: 43px;
	@include background-image(linear-gradient(#FFFFFF, #d4d4d4));
	div.branding-data {
		float: left;
		width: 30%;
		img {
			height: 25px;
			margin: 9px 0 0 0;
		}
	}
	div.block-menu-block {
		float: left;
		width: 70%;
	}
	.menu-block-1 {
		ul {
			float: right;
			position: relative;
			right: -20px;
		}
		li {
			float: left;
			padding: 12px 15px 12px;
			border-right: 1px solid #cbcbcb;
			a {
				display: block;
				width: 19px; height: 19px;
			}
		}
	}
}

/**
 * HEADER - Title
 */
.zone-header {
	height: 20px; padding: 10px 0;
	background-color: $color-red-dark;
	font-size: 16px; line-height: 20px;
	font-weight: bold; text-transform: uppercase;
	.back-btn-wrapper {
		height: 19px;
		padding: 11px 12px 10px 12px;
		margin: -10px 12px 0 -15px;
		border-right: 1px solid #a4897e;
		float: left;
		a {
			float: left; display: block;
		}
	}
}

/**
 * NODES
 */
.page-node {
	h1#page-title {
		color: #FFFFFF;
		text-align: center;
	}
	.node {
		.content {
			color: #FFFFFF;
			font-size: $font-size-std;
			line-height: $line-height-std;
			text-align: center;
		}
	}
}

.node-type-event {

	h1#page-title { display: none; }
	h2.node-title {
		padding: 12px 0;
		border-bottom: 1px solid white;
		border-top: 1px solid white;
		margin-bottom: 22px;
		font-size: 22px; line-height: 22px;	
	}
	.field-name-field-event-category {
		margin: 0 auto 30px;
	}
	.field-name-field-event-date {
		font-weight: bold;
		font-size: 17px;
		text-transform: uppercase;
		margin-bottom: 10px;
	}
	.field-name-body { font-size: 12px; }
	.field-label { text-transform: uppercase; }
}

.page-node-add {
	h1#page-title { display: none; }
}

/**
 * TABS
 */
.tabs {
	margin-bottom: 20px;
}
ul.tabs.primary {
	margin: 0;
	text-transform: uppercase;
	font-size: $font-size-std; line-height: $font-size-std;
	li {
		float: left;
		display: block;		
		text-align: center;
		margin: 0;
		width: 31%;
		a {
			background-color: $color-red-dark;
			color: #FFFFFF;
			text-decoration: none;
			display: block;
			padding: 8px 0;
			@include border-radius(3px);
		}
	}
	li.active {
		a {
			background-color: #FFFFFF;
			color: $color-red-dark;
		}
	}
	li:nth-child(1) {
		margin-right: 2%;
	}
	li:nth-child(2) {
		margin: 0 1%;
	}
	li:nth-child(3) {
		margin-left: 2%;
	}
}

/**
 * USER LOGIN PAGE
 */
.page-user.not-logged-in {
	h1#page-title { display: none; }
	.zone-branding {
		display: none;
	}
}
form#user-login {
	padding: 0 41px;
}
div#user-login-header {
	text-align: center;
	margin-bottom: 40px;
	margin-top: 40px;
	img {
		width: 192px;
		height: 98px;
		margin: auto;
	}
}
div#user-login-footer {
	text-align: center;
	color: $color-red-dark;
	font-size: 11px;
	a {
		text-transform: uppercase;
		color: #FFFFFF;
	}
}

/** 
 * USER PROFILE
 */
 
.page-user {
	#page-title {
		display: none;
	}
	#zone-header { 
		display: none; 
	}
}
.page-user-edit {
	#zone-header { 
		display: block; 
	}
}
#user-profile-form {
	.user-picture {
		float: left;
		img { @include border-radius(3px); max-width: 116px; max-height: 116px; margin: 0 10px 10px 0; }
	}
}
.profile {
	.user-picture {
		float: left;
		margin: 0 1em 0 0;
		img { @include border-radius(3px); max-width: 116px; max-height: 116px; }
	}
	.field {
		font-size: $font-size-std; line-height: 17px;
	}
	.field-name-field-user-mobile {
		margin-top: 10px;
		clear: right;
	}
	.field-email {
	}
	.field-name-field-user-mobile:after {
		content: inherit;
	}
	.field-name-field-user-mobile,
	.field-email {
		font-weight: bold;
	}
	.field-name-field-user-name {
		font-size: 16px; text-transform: uppercase; font-weight: bold;
	}
	.field-label-above {
		.field-label { float: left; }
	}
	.view {
		float: left;
		width: 100%;
		h2 {
			font-size: 22px; line-height: 22px;
			margin: 20px 0;
		}
	}
	.logout {
		float: left;
		margin-top: 20px;
	}
}
/**
 * CALENDAR
 */

.view-calendar {
	font-size: 12px;
	font-family: $font-fam-std;
	
	.date-nav-wrapper {
		position: inherit;
		.date-nav {
			position: inherit;
			ul.pager {
				li {
					padding: 0;
					position: absolute;
					border-radius: 0;
					background: transparent;
					a {
						color: #FFFFFF;
						margin: 0;
						display: block;
						height: 56px; width: 22px;
						@include box-shadow(rgba(black, 0.75) 0 0 7px);
						@include background-size(8px 13px);
						background-color: $color-red-dark;
						background-repeat: no-repeat;
						background-position: center center;
					}
				}
				li.date-prev { 
					position: absolute; left: 0; top: 200px; right: auto;
					margin: 0;
					a { 
						@include border-right-radius(3px);
						background-image: url("../images/icon-pager-prev.png");
					}
				}
				li.date-next { 
					position: absolute; right: 0; top: 200px; left: auto;
					margin: 0;
					a { 
						@include border-left-radius(3px); 
						background-image: url("../images/icon-pager-next.png");
					}
				}
			}
		}
	}
	
	/* Month View */
	.month-view {
		table {
			font-size: $font-size-std;
			font-weight: bold;
			th {
				text-transform: uppercase;
				background-color: #FFFFFF;
				border: none;
				color: $color-red-dark;
				padding: 3px 5px 8px;
			}
			td {
				text-align: center;
				color: $color-red-dark;
				padding: 0 5px;
				div.month { padding: 5px 0; }
				a {
					color: $color-red-light;
					font-weight: bold;
				}
			}
			td.today {
				background: #FFF;
				.month {
				background: $color-red-light;
				color: #FFFFFF;
				a { color: #FFFFFF; }
				}
			}
			
		}
	}	
}

/* MONTH VIEW */
.view-calendar.view-display-id-page_1 {

	.date-nav {
		padding: 0; margin-bottom: 0;
		.date-heading h3 {
			font-size: 17px;
			text-transform: uppercase;
			text-align: left;
			a {
				color: $color-red-dark;
				text-decoration: none;
			}	
		}
	}
	> .view-header {
		padding: 35px 18px 0 18px;	
		background-image: url("../images/calendar-top.png");
		background-position: top center;
		@include background-size(1024px);
	}
	> .view-content {
		background-color: #FFFFFF;
		padding: 0 18px 15px 18px;	
	}
	> .view-footer {
		padding: 0 18px 15px;
		background-image: url("../images/calendar-btm.png");
		background-position: bottom right;
		@include background-size(1024px);		
		color: $color-red-dark;
		min-height: 100px;
		font-size: 12px;
		h3 {
			font-weight: normal;
			margin-bottom: 4px;
		}
		.views-field-title {
			float: left;
			width: 100%;
		}
		span {
			text-transform: uppercase;
			font-weight: bold;
			line-height: 19px;
		}
		a {
			margin-top: 8px;
		}
	}
	.calendar-calendar {
		border-bottom: 1px solid #ddd4cf;
		padding-bottom: 16px;
	}
}

/* DAY VIEW */
.view-calendar.view-display-id-page_3,
.view-calendar.view-display-id-page_4 {
	.date-nav {
		padding: 0; margin-bottom: 13px;
		.date-heading h3 {
			font-size: 22px; line-height: 22px;
			text-transform: uppercase;
			text-align: left;
			font-weight: bold;
			color: #FFFFFF;		}
	}
	.views-row {
		font-family: $font-fam-std;
		padding: 0;
		margin-bottom: 10px;
		border-bottom: 1px solid $color-red-dark;
		border-right: 1px solid $color-red-dark;
		.ui-accordion-content {
			padding: 0 12px 12px;
			border: none;
			border-radius: 0;
			margin-bottom: 1px;
		}
		.views-field-title {
			border-radius: 0;
			background: #FFFFFF;
			border: none;
			padding: 0;
			font-size: 12px; line-height: 19px;
			a {
				color: $color-red-dark;		
				padding: 10px 10px;
			}
			> .ui-icon { display: none; }
		}
		.views-field-title.ui-state-active {
			font-weight: bold;
			text-transform: uppercase;		
		}
		.views-field-body {
			margin-bottom: 20px;
			.views-label-body { font-weight: bold; }
		}
	}
	.day-footer-msg {
		color: #FFFFFF;
		text-align: center;
		font-style: italic;
	}
}

/**
 * Videos View
 */
.page-video {
	#page-title {
		margin-bottom: 15px;
	}
}
.view-videos {
	.views-row {
		float: left;
	}
	.views-row-odd {
		margin-right: 15px;
	}
